@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix lv2ui: <http://lv2plug.in/ns/extensions/ui#>.

<https://github.com/ycollet/qamp#ui>
    a lv2ui:Qt4UI;
    lv2ui:binary <qamp_gui.so> .

<https://github.com/ycollet/qamp> a lv2:Plugin ;
    doap:name "Simple Qt amplifier" ;
    doap:license <http://usefulinc.com/doap/licenses/gpl> ;
    lv2:optionalFeature lv2:hardRtCapable ;
    lv2ui:ui <https://github.com/ycollet/qamp#ui>;

    lv2:port [
	a lv2:InputPort ;
	a lv2:ControlPort ;
	lv2:index 0 ;
	lv2:symbol "gain" ;
	lv2:name "gain" ;
	lv2:default 0.0 ;
	lv2:minimum -90.0 ;
	lv2:maximum 24.0 ;
    ] , [
	a lv2:AudioPort ;
	a lv2:InputPort ;
	lv2:index 1 ;
	lv2:symbol "in" ;
	lv2:name "in" ;
    ] , [
	a lv2:AudioPort ;
	a lv2:OutputPort ;
	lv2:index 2 ;
	lv2:symbol "out" ;
	lv2:name "out" ;
    ]
.
